<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="SwfOnCpp" package="com.sample.swfOnCpp" version="1.0.0" company="saumya Ltd." />
	<app main="Main" path="Export" file="SwfOnCpp" />

	<window width="400" height="700" if="desktop" />
	<window width="0" height="0" if="html5" />
	
	<source path="Source" />
	<source path="../../../../../6_github/StablexUI/src/" />
	<source path="../../3_haxelibs/raymp/src/" />
	<!-- <source path="../../3_haxelibs/gessie-develop/src/" /> -->
	
	<haxelib name="openfl" />
	<haxelib name="actuate" />

	
	<!-- <haxelib name="swf" /> --> <!-- As the SWF lib is part of OpenFL now -->
	<library path="Assets/OpenFlOne.swf" type="swflite" preload="true" generate="true" />
	<library path="Assets/Calc_Asset.swf" type="swflite" preload="true" generate="true" />
	
	<assets path="Assets" rename="assets" exclude="openfl.svg|*.swf" />
	
	<!-- Fonts -->
	<assets path="Assets/fonts" rename="fonts" include="*.ttf" />
	
</project>